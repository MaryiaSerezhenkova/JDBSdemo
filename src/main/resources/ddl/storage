-- Table: app.storage

-- DROP TABLE IF EXISTS app.storage;

CREATE TABLE IF NOT EXISTS app.storage
(
    id bigint NOT NULL,
    product bigint NOT NULL,
    region bigint NOT NULL,
    quantity bigint,
    dt_create timestamp(3) without time zone NOT NULL,
    dt_update timestamp(3) without time zone,
    CONSTRAINT storage_pkey PRIMARY KEY (id),
    CONSTRAINT product_fk FOREIGN KEY (product)
        REFERENCES app.product (id) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE NO ACTION,
    CONSTRAINT region_fk FOREIGN KEY (region)
        REFERENCES app.region (id) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE NO ACTION
)

TABLESPACE pg_default;

ALTER TABLE IF EXISTS app.storage